        .alert {
            background-color: #adf69b;
            border: 1px solid #cff5c6;
            color: #ffffff;
            padding: 10px 15px;
            border-radius: 4px;
            margin-bottom: 20px;
            font-size: smaller;
        }
            .button {
              display: inline-block;
              background-color: #007bff;
              color: #fff;
              text-decoration: none;
              padding: 10px 20px;
              border-radius: 4px;
              margin-top: 20px;
            }
            .button:hover {
              background-color: #0056b3;
            }
